<% @data.each do |obj| %>
	<h3 class="top_title"><a href="/events/show/<%= obj.id %>">
		<%= obj.title %></a></h3>

	<hr>
<% end %>
<div class="navigate">
	<% if @page_num > 0 then %>
		<a href="/events/<%= @page_num - 1 %>">＜＜前のページ</a>
	<% end %>
	<span>　</span>
	<a href="/events/<%= @page_num + 1 %>">次のページ＞＞</a>
</div>

