<p>イベントカテゴリ一覧</p>
<table>
    <tr>
        <th>Id</th><th>カテゴリ名</th><th>備考</th><th></th>
    </tr>
    <% @data.each do |obj| %>
        <tr>
            <td><%= obj.id %></td>
            <td><%= obj.name %></td>
            <td><%= obj.memo %></td>
            <td><a href="/event_categories/<%= obj.id %>">[編集]</a></td>
        </tr>
    <% end %>
</table>
<p><a href="/event_categories/add">新規作成>> </a></p>